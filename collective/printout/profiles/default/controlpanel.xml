<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.printout"
    purge="False">

    <configlet
        title="Printout"
        action_id="printout"
        appId="collective.printout"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@printout-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>